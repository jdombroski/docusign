{
    "name": "jdombroski/docusign",
    "description": "DocuSign Rest API Client for Laravel 5",
    "keywords": ["laravel", "docusign", "laravel 5", "sign", "documents", "envelope"],
    "homepage": "https://github.com/jdombroski/docusign",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeff Dombroski",
            "email": "dombroski.jeffrey@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "~6.0",
        "laravel/framework": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0@dev",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "jdombroski\\DocuSign\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jdombroski\\DocuSign\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "jdombroski\\DocuSign\\DocuSignServiceProvider"
            ]
        }
    }
}
